#include<iostream>
#include<cmath>
using namespace std;
 
void sum()

//sum
{
	int a=3,b=4;
	cout<<a+b<<endl;
}//diff
void diff()
{
	int a=3,b=4;
	cout<<a-b<<endl;
}//prod
void prod()
{
		int a=3,b=4;
	cout<<a*b<<endl;
}//qout
void quot()
{
	int a=3,b=4;
	cout<<a/b<<endl;
}//sqrt
void sqrt()
{
	int a=3;
	cout<<sqrt(a)<<endl;
}//log
void log()
{
	int g=9;
	cout<<log(g)<<endl;
}//remainder
void remain()
{
	int c=10,d=79;
	cout<<c%d<<endl;
}
int main()
{
	sum();
	 diff();
	   prod();
	     quot();
	   	    sqrt();
	   	        log();
	   	        remain();
}
